										<ADDACTIONLIST NAME="AvisportsWorkflow" />
										<ADDACTIONLIST NAME="UpdateBookmarks"/>
										<ADDACTIONLIST NAME="AddMarketingTab"/>
										<ADDACTIONLIST NAME="InstallYouTubeSample"/>
										<IF COND="Variables.oracle.wcsites.examples.blogs=true">
											<THEN>
												<ADDACTIONLIST NAME="ImportBlogInstallerElements" />
												<ADDACTIONLIST NAME="ImportBlogElements" />
												<ADDACTIONLIST NAME="ImportBlogData" />
												<ADDACTIONLIST NAME="ShareBlogsDataWithSelectedSite"/>
												<ADDACTIONLIST NAME="ShareBlogsWithSelectedSite"/>
											</THEN>
										</IF>
										<ADDACTIONLIST NAME="RemoveInstallationElements"/>
									</DIALOGACTION>
								</THEN>
							</IF>
						</THEN>
					</IF>
				</THEN>
			</IF>
