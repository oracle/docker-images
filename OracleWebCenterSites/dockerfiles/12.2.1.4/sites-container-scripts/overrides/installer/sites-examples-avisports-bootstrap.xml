<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CSINSTALL SYSTEM "sites-bootstrap.dtd">
<CSINSTALL TITLE="Sample Sites" FIRSTPACKAGE="AvisportsInstallPage">
