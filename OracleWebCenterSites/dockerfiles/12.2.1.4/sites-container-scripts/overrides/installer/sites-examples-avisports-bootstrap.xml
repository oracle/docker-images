										<THEN>
											<IF COND="Variables.oracle.wcsites.ldap.servertype=WEBLOGICLDAP">
												<THEN>
													<DIALOGACTION>
														<ADDACTIONLIST NAME="ImportIdcsAviSportsInstallerElements"/>
													</DIALOGACTION>
												</THEN>
											</IF>
										</THEN>
									</IF>										
									<DIALOGACTION>
										<ADDACTIONLIST NAME="DeployAvisportsData"/>
										<ADDACTIONLIST NAME="CopyCGIntegrationFilesForAvisports"/>
										<ADDACTIONLIST NAME="CopyYouTubeSample"/>
										<ADDACTIONLIST NAME="ImportInstallationElementsServer"/>
										<ADDACTIONLIST NAME="ConfigureWebRootForAvisports"/>
										<ADDACTIONLIST NAME="InstallAvisports"/>
										<ADDACTIONLIST NAME="ImportCatalogsAvisports"/>
										<ADDACTIONLIST NAME="SetPropertiesForNewInstall"/>
