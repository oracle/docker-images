			<IF COND="Variables.oracle.wcsites.management=true">
				<THEN>
					<IF COND="Variables.oracle.wcsites.examples=true">
						<THEN>
							<IF COND="Variables.oracle.wcsites.examples.avisports=true">
								<THEN>
